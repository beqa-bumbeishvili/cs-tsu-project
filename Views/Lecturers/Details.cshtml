@model ComputerScienceTsu.Models.Generated.Lecturer

<section id="news">
    <div class="container">
        <div class="content">
            <div class="row">
                <div class="col-lg-3 col-md-4 bottom-sm">
                    <nav class="box-white no-padding">
                        <ul class="side-menu">
                            <li class="sidemenu-item"><a href="#">სიახლეები</a></li>
                            <li class="sidemenu-item has-dropdown">
                                <a>კათედრების სია&nbsp;&nbsp;<i class="fa fa-caret-down"></i></a>
                                <ul class="side-submenu">
                                    <li class="submenu-item"><a href="#">გამოყენებითი ინფორმატიკა</a></li>
                                    <li class="submenu-item"><a href="#">თეორიული ინფორმატიკა</a></li>
                                    <li class="submenu-item"><a href="#">პრაქტიკული ინფორმატიკა</a></li>
                                    <li class="submenu-item"><a href="#">ტექნიკური ინფორმატიკა</a></li>
                                </ul>
                            </li>
                            <li class="sidemenu-item"><a href="#">ლაბორატორიები</a></li>
                            <li class="sidemenu-item"><a href="#">ინსტიტუტები</a></li>
                            <li class="sidemenu-item has-dropdown">
                                <a>სწავლება&nbsp;&nbsp;<i class="fa fa-caret-down"></i></a>
                                <ul class="side-submenu">
                                    <li class="submenu-item"><a href="#">საბაკალავრო პროგრამები</a></li>
                                    <li class="submenu-item"><a href="#">სამაგისტრო პროგრამები</a></li>
                                    <li class="submenu-item"><a href="#">სადოქტორო პროგრამები</a></li>
                                    <li class="submenu-item"><a href="#">სასწავლო/საწარმოო პრაქტიკა</a></li>
                                </ul>
                            </li>
                            <li class="sidemenu-item has-dropdown">
                                <a>კვლევა&nbsp;&nbsp;<i class="fa fa-caret-down"></i></a>
                                <ul class="side-submenu">
                                    <li class="submenu-item"><a href="#">კვლევის მიმართულებები</a></li>
                                    <li class="submenu-item"><a href="#">კონფერენციები</a></li>
                                    <li class="submenu-item"><a href="#">გრანტები</a></li>
                                    <li class="submenu-item"><a href="#">ექსპედიციები</a></li>
                                    <li class="submenu-item"><a href="#">დაცული დისერტაციები</a></li>
                                    <li class="submenu-item"><a href="#">მიმდინარე მიზნობრივი პროექტები</a></li>
                                    <li class="submenu-item"><a href="#">სტუდენტთა სამეცნიერო წრე</a></li>
                                </ul>
                            </li>
                            <li class="sidemenu-item"><a href="">ფოტოგალერეა</a></li>
                            <li class="sidemenu-item"><a href="">კონტაქტი</a></li>
                        </ul>
                    </nav>
                </div>


                <div class="col-lg-6 col-md-8">
                    <div class="box-white">

                        <div class="lecturer-inf clearfix">

                            <div class="lecturer-avatar">
                                <img src="@Html.DisplayFor(model => model.Photo.Image_path)" class="img-thumbnail">
                            </div>

                            <h2 class="lecturer-name">@Html.DisplayFor(model => model.Name) @Html.DisplayFor(model => model.Lastname)</h2>
                            <h4 class="lecturer-profession">ასოცირებული პროფესორი</h4>

                            <ul class="lecturer-list">
                                <li><i class="fa fa-home"></i>&nbsp;@Html.DisplayFor(model => model.Full_address)</li>
                                <li style="text-align:right;"> <i class="fa fa-phone"></i>&nbsp;@Html.DisplayFor(model => model.Phone)</li>
                                <li style="text-align:right;"><i class="fa fa-envelope-o"></i>&nbsp;<a href="mailto:@Html.DisplayFor(model => model.Email)">@Html.DisplayFor(model => model.Email)</a></li>
                            </ul>

                        </div>

                        <div id="tabs">

                            <!-- Nav tabs -->
                            <ul class="nav nav-tabs" role="tablist">
                                <li role="presentation" class="active"><a href="#home" aria-controls="home" role="tab" data-toggle="tab">კურსები</a></li>
                                <li role="presentation"><a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">პუბლიციები</a></li>
                                <li role="presentation"><a href="#messages" aria-controls="messages" role="tab" data-toggle="tab">დამატებითი ინფო</a></li>
                                <li role="presentation"><a href="#settings" aria-controls="settings" role="tab" data-toggle="tab">@Html.DisplayFor(model => model.Name)-ს შესახებ</a></li>
                            </ul>

                            <!-- Tab panes -->
                            <div class="tab-content">
                                <div role="tabpanel" class="tab-pane active" id="home">
                                    <div class="tab-content">
                                    </div>
                                </div>
                                <div role="tabpanel" class="tab-pane" id="profile">
                                    @foreach (var item in Model.Publications)
                                    {
                                        <p style="margin-left:2%;"><a href="/Publications/Details/@item.ID">@item.Title</a></p>
                                    }

                                </div>
                                <div role="tabpanel" class="tab-pane" id="messages">@Html.DisplayFor(model => model.Additional_info)</div>
                                <div role="tabpanel" class="tab-pane" id="settings">@Html.DisplayFor(model => model.About)</div>
                            </div>

                        </div>

                    </div>
                </div>

                <div class="col-lg-3 hidden-sm hidden-xs hidden-md">
                    <div class="box-white sidebar-hotlinks">

                        <a href="http://tsu.edu.ge/ge/faculties/science/ojyqp0d9tyfxg2m/">
                            <img class="img-responsive" src="https://www.tsu.ge/data/image_db_innova/faculty_zust/abi.jpg" alt="">
                        </a>

                        <a href="http://tsu.edu.ge/ge/faculties/science/yoqrmvp_gctzkw9mk/">
                            <img class="img-responsive" src="https://www.tsu.ge/data/image_db_innova/faculty_zust/sandiego-uni.png" alt="">
                        </a>

                        <a href="https://www.tsu.ge/ge/faculties/science/gcxwjek_yvqyft0g4/">
                            <img class="img-responsive" src="https://www.tsu.ge/data/image_db_innova/faculty_zust/geo-german.png" alt="">
                        </a>

                        <a href="https://www.tsu.ge/ge/faculties/science/aybwuvm7mmpi70j9o/">
                            <img class="img-responsive" src="https://www.tsu.ge/data/image_db_innova/faculty_zust/baneri-STM.png" alt="">
                        </a>

                        <a href="https://www.tsu.ge/ge/faculties/science/iglb0tdf6hhohskx/">
                            <img class="img-responsive" src="https://www.tsu.ge/data/image_db_innova/faculty_zust/projects-1.png" alt="">
                        </a>

                        <a href="https://www.tsu.ge/ge/faculties/science/_wfplofnpxmqm4qwr/">
                            <img class="img-responsive" src="https://www.tsu.ge/data/image_db_innova/faculty_zust/banner-viktorina.png" alt="">
                        </a>

                        <a href="http://www.ffi.tsu.ge/">
                            <img class="img-responsive" src="https://www.tsu.ge/data/image_db_innova/ffi.gif" alt="">
                        </a>

                        <a href="https://www.tsu.ge/ge/faculties/science/jqvn8ssyelvp0-lyn/">
                            <img class="img-responsive" src="https://www.tsu.ge/data/image_db_innova/faculty_zust/gacvliTi.jpg" alt="">
                        </a>

                    </div>
                </div>
            </div>
        </div>

    </div>
</section>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.ID }) |
    @Html.ActionLink("Back to List", "Index")
</p>
