                    <div class="col-lg-6 col-md-8">
                        <div class="box-white">
                            <h3 class="some-heading">რეგისტრაცია</h3>

                            <form action="@Url.Action("Registration", "User")" method="post" name="registrationForm" onSubmit="return formValidation();">
                                <div class="form-group">
                                    <label for="exampleInputName">სახელი და გვარი</label>
                                    <div class="row">
                                        <div class="col-sm-6 bottom-sm">
                                            <div class="has-icon">
                                                <input type="text" name="firstname" placeholder="სახელი" class="form-control" id="exampleInputName">
                                                <span class="form-icon"><i class="fa fa-user"></i></span>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="has-icon">
                                                <input type="text" name="lastname" placeholder="გვარი" class="form-control">
                                                <span class="form-icon"><i class="fa fa-address-card"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="exampleInputEmail1">იუზერი</label>
                                    <div class="has-icon">
                                        <input type="text" name="username" class="form-control" placeholder="შეიყვანეთ იუზერის სახელი">
                                        <span class="form-icon"><i class="fa fa-user-circle"></i></span>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="exampleInputEmail1">ელ. ფოსტა</label>
                                    <div class="has-icon">
                                        <input type="email" name="email" class="form-control" id="exampleInputEmail1" placeholder="შეიყვანეთ ელ. ფოსტა">
                                        <span class="form-icon"><i class="fa fa-at"></i></span>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputPassword1">პაროლი</label>
                                    <div class="has-icon">
                                        <input type="password" name="pass" class="form-control" id="exampleInputPassword1" placeholder="შეიყვანეთ პაროლი">
                                        <span class="form-icon"><i class="fa fa-key"></i></span>
                                    </div>

                                </div>
                                <div class="form-group">
                                    <label for="exampleInputPassword2">გაიმეორეთ პაროლი</label>
                                    <div class="has-icon">
                                        <input type="password" name="repeatPass" class="form-control" id="exampleInputPassword2" placeholder="გაიმეორეთ პაროლი">
                                        <span class="form-icon"><i class="fa fa-key"></i></span>
                                    </div>
                                </div>

                                <div class="checkbox">
                                    <label>
                                        <input type="checkbox"> ვეთანხმები ვებ-გვერდის <a href="#">მომსახურების პირობებს</a>
                                    </label>
                                </div>

                                <div class="text-center">
                                    <button type="submit" onclick="validations(document.registrationForm)" class="btn btn-primary">რეგისტრაცია</button>
                                </div>
                            </form>

                        </div>

